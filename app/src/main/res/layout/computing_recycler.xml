<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ComputingRecycler">

    <androidx.recyclerview.widget.RecyclerView

        android:id="@+id/rv_computing"
        android:layout_width="443dp"
        android:layout_height="493dp"
        android:layout_below="@+id/textView38"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginTop="61dp"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginBottom="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView38" />

    <TextView
        android:id="@+id/textView38"
        android:layout_width="232dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/iv_computingrecycler_home"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="91dp"
        android:layout_marginLeft="91dp"
        android:layout_marginTop="94dp"
        android:layout_marginEnd="88dp"
        android:layout_marginRight="88dp"
        android:fontFamily="serif"
        android:gravity="center"
        android:text="Computing Books"
        android:textColor="#009688"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintTop_toBottomOf="@+id/iv_computingrecycler_home"
        tools:layout_editor_absoluteX="143dp" />

    <ImageView
        android:id="@+id/iv_computingrecycler_home"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginStart="124dp"
        android:layout_marginLeft="124dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="184dp"
        android:layout_marginRight="184dp"
        android:layout_toEndOf="@+id/iv_computing_back"
        android:layout_toRightOf="@+id/iv_computing_back"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/images"
        tools:layout_editor_absoluteY="16dp" />

    <ImageView
        android:id="@+id/iv_computing_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="39dp"
        app:srcCompat="@drawable/ic_baseline_my_arrow_back_24"
        tools:layout_editor_absoluteX="33dp"
        tools:layout_editor_absoluteY="38dp" />

</RelativeLayout>